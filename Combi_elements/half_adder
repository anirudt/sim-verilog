#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_00833EB8 .scope module, "stimulus" "stimulus" 2 15;
 .timescale 0 0;
v0083DAE0_0 .var "a", 0 0;
v0083DB38_0 .var "b", 0 0;
v0083DB90_0 .var "c_in", 0 0;
v0083DBE8_0 .net "c_out", 0 0, v00831668_0; 1 drivers
v0083DC40_0 .var "clk", 0 0;
v0083DC98_0 .net "sum", 0 0, v00831718_0; 1 drivers
S_0083A1A8 .scope module, "test" "half_adder" 2 28, 2 1, S_00833EB8;
 .timescale 0 0;
v00831610_0 .net "a", 0 0, v0083DAE0_0; 1 drivers
v008391F8_0 .net "b", 0 0, v0083DB38_0; 1 drivers
v00839250_0 .net "c_in", 0 0, v0083DB90_0; 1 drivers
v00831668_0 .var "c_out", 0 0;
v008316C0_0 .net "clk", 0 0, v0083DC40_0; 1 drivers
v00831718_0 .var "sum", 0 0;
E_00838608 .event negedge, v008316C0_0;
    .scope S_0083A1A8;
T_0 ;
    %wait E_00838608;
    %load/v 8, v00831610_0, 1;
    %load/v 9, v008391F8_0, 1;
    %xor 8, 9, 1;
    %load/v 9, v00839250_0, 1;
    %xor 8, 9, 1;
    %set/v v00831718_0, 8, 1;
    %load/v 8, v00831610_0, 1;
    %load/v 9, v008391F8_0, 1;
    %and 8, 9, 1;
    %load/v 9, v008391F8_0, 1;
    %load/v 10, v00839250_0, 1;
    %and 9, 10, 1;
    %or 8, 9, 1;
    %load/v 9, v00831610_0, 1;
    %load/v 10, v00839250_0, 1;
    %and 9, 10, 1;
    %or 8, 9, 1;
    %set/v v00831668_0, 8, 1;
    %jmp T_0;
    .thread T_0;
    .scope S_00833EB8;
T_1 ;
    %vpi_call 2 22 "$dumpfile", "half_adder.vcd";
    %vpi_call 2 23 "$dumpvars";
    %end;
    .thread T_1;
    .scope S_00833EB8;
T_2 ;
    %set/v v0083DC40_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_00833EB8;
T_3 ;
    %delay 5, 0;
    %load/v 8, v0083DC40_0, 1;
    %inv 8, 1;
    %set/v v0083DC40_0, 8, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_00833EB8;
T_4 ;
    %set/v v0083DAE0_0, 0, 1;
    %set/v v0083DB38_0, 0, 1;
    %set/v v0083DB90_0, 1, 1;
    %delay 5, 0;
    %set/v v0083DAE0_0, 1, 1;
    %set/v v0083DB38_0, 0, 1;
    %set/v v0083DB90_0, 0, 1;
    %delay 20, 0;
    %set/v v0083DAE0_0, 0, 1;
    %set/v v0083DB38_0, 1, 1;
    %set/v v0083DB90_0, 0, 1;
    %delay 40, 0;
    %set/v v0083DAE0_0, 1, 1;
    %set/v v0083DB38_0, 1, 1;
    %set/v v0083DB90_0, 0, 1;
    %delay 60, 0;
    %set/v v0083DAE0_0, 1, 1;
    %set/v v0083DB38_0, 1, 1;
    %set/v v0083DB90_0, 1, 1;
    %vpi_call 2 52 "$finish";
    %end;
    .thread T_4;
    .scope S_00833EB8;
T_5 ;
    %vpi_call 2 57 "$monitor", $time, "The sum is %b and the carry is %b", v0083DC98_0, v0083DBE8_0;
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "half_adder.v";
